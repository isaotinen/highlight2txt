{
  "manifest_version": 2,
  "name": "highlight2txt",
  "version": "1.0",
  "description": "Convert Kindle highlights to text",
  "permissions": [
    "tabs","activeTab"
  ],
  "icons" : {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"]
    }
    ]
}
